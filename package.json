{
  "name": "@cldcvr/flow-icon",
  "version": "0.0.4",
  "description": "This package contains free set icons to use in `@cldcvr/flow-core`",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "workspaces": [
    "packages/@cldcvr/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
