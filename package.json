    "publish-packages": "npx lerna publish patch --no-private --yes"
  },
  "keywords": [
    "icons"
  ],
  "author": "@cldcvr",
  "license": "MIT",
  "dependencies": {
    "@cldcvr/flow-core": "*"
  },
  "devDependencies": {
    "@types/node": "^18.7.17",
    "@types/prettier": "^2.7.0",
    "axios": "^0.27.2",
    "fs": "^0.0.1-security",
    "lerna": "^5.5.1",
    "lit-html": "^2.2.7",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@cldcvr/flow-core": "*"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cldcvr/flow-icon.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://pkgs.dev.azure.com/cldcvrdevusa/_packaging/FlowLibraryFeed/npm/registry/",
    "always-auth": true
